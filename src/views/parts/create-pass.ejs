<section>
  <h2>Crie sua própria sala</h2>
  <form method="POST" action="/create-room">
    <label class="sr-only" for="room-id">Código da sala</label>
    <input
      type="number"
      name="password"
      id="room-id"
      placeholder="Código da Sala"
    />
    <button>
      <img src="images/icon-users-white.svg" alt="Criar sala" />Criar sala
    </button>
  </form>
</section>
